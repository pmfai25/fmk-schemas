<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20090101="http://www.dkma.dk/medicinecard/xml.schema/2009/01/01" xmlns:medicinecard20080601="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" xmlns:receptserver="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2009/01/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_DrugMedicationIdentifier.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_DrugMedicationVersionIdentifier.xsd"/>
	<include schemaLocation="DKMA_ModifiedStructure.xsd"/>
	<include schemaLocation="DKMA_CreatedStructure.xsd"/>
	<include schemaLocation="DKMA_PausedStructure.xsd"/>
	<include schemaLocation="DKMA_IndicationStructure.xsd"/>
	<include schemaLocation="DKMA_NegativeConsentIndicator.xsd"/>
	<include schemaLocation="DKMA_PriceListVersionDate.xsd"/>
	<include schemaLocation="DKMA_PriceListVersionWeek.xsd"/>	
	<include schemaLocation="DKMA_DrugMedicationBeginEndDateStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_CreatedDateTime.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_ModifiedDateTime.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_DoctorStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_RouteOfAdministrationStructure.xsd"/>
	<include schemaLocation="DKMA_DrugStructure.xsd"/>
	<include schemaLocation="DKMA_DosageStructure.xsd"/>
	<include schemaLocation="DKMA_DosageStructureTranslation.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_SubstitutionAllowed.xsd"/>
	<include schemaLocation="DKMA_EffectuationStructure.xsd"/>
    <import namespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" schemaLocation="../../../2008/06/01/receptserver-schemas/DKMA_NewPrescriptionMedicationStructure.xsd"/>
	<element name="NewDrugMedicationStructure" type="medicinecard20090101:NewDrugMedicationStructureType">
		<annotation>
			<documentation xml:lang="en-GB">Specification of which drug to use (medication), how much to give (dosage and quantity), for how long (start- and enddate), how to adminster it (route of administration) and the cause (indication)</documentation>
			<documentation xml:lang="da-Dk">Anvisning af, hvilken medicin der skal anvendes (lægemiddel) hvor meget der skal gives (lægemiddeldosering), hvor længe (startdato og slutdato), hvordan den skal tilføres (administrationsvej) og hvorfor (indikation)</documentation>
		</annotation>
	</element>
	<complexType name="NewDrugMedicationStructureType">
		<sequence>
			<element ref="medicinecard20080601:DrugMedicationIdentifier"/>
			<element ref="medicinecard20080601:DrugMedicationVersionIdentifier"/>
			<element ref="medicinecard20090101:ModifiedStructure" minOccurs="0"/>
			<element ref="medicinecard20090101:CreatedStructure"/>
			<element ref="medicinecard20090101:PausedStructure" minOccurs="0"/>
			<element ref="medicinecard20090101:NegativeConsentIndicator" minOccurs="0"/>
			<choice>
				<element ref="medicinecard20090101:PriceListVersionDate"/>
				<element ref="medicinecard20090101:PriceListVersionWeek"/>
			</choice>
			<element ref="medicinecard20090101:DrugMedicationBeginEndDateStructure"/>
			<element ref="medicinecard20090101:IndicationStructure"/>
			<element ref="medicinecard20080601:RouteOfAdministrationStructure"/>
			<element ref="medicinecard20090101:DrugStructure"/>
			<element ref="medicinecard20090101:DosageStructure"/>
			<element ref="medicinecard20090101:DosageStructureTranslation" minOccurs="0"/>
			<element ref="medicinecard20080601:SubstitutionAllowed" minOccurs="0"/>
			<element ref="medicinecard20090101:EffectuationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="receptserver:NewPrescriptionMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>
