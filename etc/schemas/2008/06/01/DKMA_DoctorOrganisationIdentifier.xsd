<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="DoctorOrganisationIdentifier" type="medicinecard:DoctorOrganisationIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">Identification number of the organisation of the precribing doctor in case of medical practitioner number</documentation>
			<documentation xml:lang="da-DK">Identifikationsnummer for den ordinerende laeges organisation saafremt dette er et ydernummer</documentation>
		</annotation>
	</element>
	<simpleType name="DoctorOrganisationIdentifierType">
		<restriction base="int">
			<minInclusive value="1"/>
			<maxExclusive value="999999"/>
		</restriction>
	</simpleType>
</schema>
