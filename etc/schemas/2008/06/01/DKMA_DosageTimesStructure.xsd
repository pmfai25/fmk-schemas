<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DKMA_DosageTimesIterationIntervalQuantity.xsd"/>
	<include schemaLocation="DKMA_DosageTimesStartDate.xsd"/>
	<include schemaLocation="DKMA_DosageTimesEndDate.xsd"/>
	<include schemaLocation="DKMA_DosageQuantityUnitText.xsd"/>
	<include schemaLocation="DKMA_DosageDayElementStructure.xsd"/>
	<element name="DosageTimesStructure" type="medicinecard:DosageTimesStructureType">
		<annotation>
			<documentation xml:lang="en-GB">Description of the dosage times of a treatment</documentation>
			<documentation xml:lang="da-DK">Beskrivelse af doseringstidspunkter for en behandling</documentation>
		</annotation>
	</element>
	<complexType name="DosageTimesStructureType">
		<sequence>
			<element ref="medicinecard:DosageTimesIterationIntervalQuantity"/>
			<element ref="medicinecard:DosageTimesStartDate"/>
			<element ref="medicinecard:DosageTimesEndDate" minOccurs="0"/>
			<element ref="medicinecard:DosageQuantityUnitText"/>
			<element ref="medicinecard:DosageDayElementStructure" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>
