<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DKMA_DosageTimesStructure.xsd"/>
	<include schemaLocation="DKMA_DosageFreeText.xsd"/>
	<include schemaLocation="DKMA_AdministrationAccordingToSchemeInLocalSystemIndicator.xsd"/>
	<element name="DosageStructure" type="medicinecard:DosageStructureType">
		<annotation>
			<documentation xml:lang="en-GB">How often and when the drug should be given</documentation>
			<documentation xml:lang="da-DK">Angivelse af hvor ofte og hvornår et lægemiddel skal gives</documentation>
		</annotation>
	</element>
	<complexType name="DosageStructureType">
		<choice>
			<element ref="medicinecard:DosageTimesStructure"/>
			<element ref="medicinecard:DosageFreeText"/>
			<element ref="medicinecard:AdministrationAccordingToSchemeInLocalSystemIndicator"/>
		</choice>
	</complexType>
</schema>
