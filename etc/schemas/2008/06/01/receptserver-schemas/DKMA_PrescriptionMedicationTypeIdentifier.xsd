<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" targetNamespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="PrescriptionMedicationTypeIdentifier" type="medicinecard:PrescriptionMedicationTypeIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">The type of prescription medication</documentation>
			<documentation xml:lang="da-DK">Ordinationens type, engangsudlevering, reitereret udlevering eller dosisdispenseret</documentation>
		</annotation>
	</element>
	<simpleType name="PrescriptionMedicationTypeIdentifierType">
		<restriction base="string">
			<enumeration value="engangsudlevering"/>
			<enumeration value="reitereret udlevering"/>
			<enumeration value="dosisdispenering"/>
		</restriction>
	</simpleType>
</schema>
