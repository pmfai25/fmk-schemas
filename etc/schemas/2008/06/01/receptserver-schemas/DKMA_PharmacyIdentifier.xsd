<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" targetNamespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="PharmacyIdentifier" type="medicinecard:PharmacyIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">Identification number of the Pharmacy</documentation>
			<documentation xml:lang="da-DK">Identifikationsnummer (P-nummer) for apoteket</documentation>
		</annotation>
	</element>
	<simpleType name="PharmacyIdentifierType">
		<restriction base="long">
			<minInclusive value="1000000000"/>
			<maxExclusive value="9999999999"/>
		</restriction>
	</simpleType>
</schema>
