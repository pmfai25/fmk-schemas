<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" targetNamespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="DrugStrengthValue" type="medicinecard:DrugStrengthValueType">
		<annotation>
			<documentation xml:lang="en-GB">Drug strength, numerical value (DMA01 field 11)</documentation>
			<documentation xml:lang="da-DK">Lægemiddelstyrke, numerisk værdi (LMS01 felt 11)</documentation>
		</annotation>
	</element>
    <simpleType name="DrugStrengthValueType">
        <restriction base="decimal">
            <fractionDigits value="3"/>
            <totalDigits value="10"/>
            <minInclusive value="0.000"/>
        </restriction>
    </simpleType>
</schema>
