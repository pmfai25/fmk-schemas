<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:receptserver="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" xmlns:medicinecard20090101="http://www.dkma.dk/medicinecard/xml.schema/2009/01/01" xmlns:medicinecard20080601="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DKMA_PrescriptionMedicationIdentifier.xsd"/>
	<include schemaLocation="DKMA_CreatedStructure.xsd"/>
	<include schemaLocation="DKMA_IndicationStructure.xsd"/>
	<include schemaLocation="DKMA_ReceiptDosageStructure.xsd"/>
	<include schemaLocation="DKMA_PriceListVersionDate.xsd"/>
	<include schemaLocation="DKMA_PrescriptionMedicationTypeIdentifier.xsd"/>
	<include schemaLocation="DKMA_RouteOfAdministrationStructure.xsd"/>
	<include schemaLocation="DKMA_DrugStructure.xsd"/>
	<include schemaLocation="DKMA_LatestEffectuationDateTime.xsd"/>
	<include schemaLocation="DKMA_TerminatedDateTime.xsd"/>
	<include schemaLocation="DKMA_CreatedLocalDateTime.xsd"/>
    <import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../DKMA_PackageNumberIdentifier.xsd"/>

	<element name="NewPrescriptionMedicationOverviewStructure" type="receptserver:NewPrescriptionMedicationOverviewStructureType">
		<annotation>
			<documentation xml:lang="en-GB">An overview of the prescription medication (a medication on a prescription)</documentation>
			<documentation xml:lang="da-DK">En oversigt over en receptordination (en ordination på en recept)</documentation>
		</annotation>
	</element>
	<complexType name="NewPrescriptionMedicationOverviewStructureType">
		<sequence>
			<element ref="receptserver:PrescriptionMedicationIdentifier"/>
			<element ref="receptserver:CreatedStructure"/>
			<element ref="receptserver:PriceListVersionDate" minOccurs="0"/>
			<element ref="receptserver:CreatedLocalDateTime" minOccurs="0"/>
			<element ref="receptserver:LatestEffectuationDateTime" minOccurs="0"/>
			<element ref="receptserver:TerminatedDateTime" minOccurs="0"/>
			<element ref="receptserver:IndicationStructure" minOccurs="0"/>
			<element ref="receptserver:ReceiptDosageStructure" minOccurs="0"/>
			<element ref="receptserver:RouteOfAdministrationStructure" minOccurs="0" maxOccurs="4"/>
			<element ref="receptserver:PrescriptionMedicationTypeIdentifier"/>
			<element ref="receptserver:DrugStructure"/>
            <element ref="medicinecard20080601:PackageNumberIdentifier" minOccurs="0"/>
		</sequence>
	</complexType>
</schema>
