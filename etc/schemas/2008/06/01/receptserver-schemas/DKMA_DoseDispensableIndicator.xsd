<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" targetNamespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="DoseDispensableIndicator" type="medicinecard:DoseDispensableIndicatorType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if the drug is dose dispensable. If "true" the drug can be dose dispensed</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt lægemidlet kan dosisdispenseres. Lægemidlet må dosisdispenseres hvis feltet er sat til true</documentation>
		</annotation>
	</element>
	<simpleType name="DoseDispensableIndicatorType">
		<restriction base="boolean"/>
	</simpleType>
</schema>
