<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" targetNamespace="http://dkma.dk/receptserver/apotekssnitflade/xml/schemas/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="HospitalOrganisationIdentifier" type="medicinecard:HospitalOrganisationIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">Identification number of the organisation of the precribing doctor in case of a hospital number, section number etc</documentation>
			<documentation xml:lang="da-DK">Identifikationsnummer for den ordinerende læges organisation såfremt dette er et SKS sygehus nummer, afdelingsnummer mm</documentation>
		</annotation>
	</element>
	<simpleType name="HospitalOrganisationIdentifierType">
		<restriction base="string">
			<minLength value="1"/>
			<maxLength value="17"/>
		</restriction>
	</simpleType>
</schema>
