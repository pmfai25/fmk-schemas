<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="HospitalOrganisationIdentifier" type="medicinecard:HospitalOrganisationIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">Identification number of the organisation of the precribing doctor in case of a hospital number, section number etc</documentation>
			<documentation xml:lang="da-DK">Identifikationsnummer for den ordinerende laeges organisation saafremt dette er et SKS sygehus nummer, afdelingsnummer mm</documentation>
		</annotation>
	</element>
	<simpleType name="HospitalOrganisationIdentifierType">
		<restriction base="string">
			<minLength value="4"/>
			<maxLength value="7"/>
			<pattern value="[0-9A-Z]{4}|[0-9A-Z]{6}|[0-9A-Z]{7}"/>
		</restriction>
	</simpleType>
</schema>
