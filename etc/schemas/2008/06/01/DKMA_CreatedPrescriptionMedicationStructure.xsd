<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DKMA_DrugMedicationIdentifier.xsd"/>
	<include schemaLocation="DKMA_DrugMedicationVersionIdentifier.xsd"/>
	<include schemaLocation="DKMA_VersionMismatchWarningIndicator.xsd"/>
	<include schemaLocation="DKMA_PrescriptionMedicationIdentifier.xsd"/>
	<element name="CreatedPrescriptionMedicationStructure" type="medicinecard:CreatedPrescriptionMedicationStructureType">
		<annotation>
			<documentation xml:lang="en-GB">Document containing one or more prescriptions medications created on a drug prescription</documentation>
			<documentation xml:lang="da-DK">Dokument der indeholder en eller flere receptordinationer der er oprettet på en lægemiddelordination</documentation>
		</annotation>
	</element>
	<complexType name="CreatedPrescriptionMedicationStructureType">
		<sequence>
			<element ref="medicinecard:DrugMedicationIdentifier"/>
			<element ref="medicinecard:PrescriptionMedicationIdentifier" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>
