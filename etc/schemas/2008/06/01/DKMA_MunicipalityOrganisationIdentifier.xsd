<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="MunicipalityOrganisationIdentifier" type="medicinecard:MunicipalityOrganisationIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">Identification number of the organisation of the precribing doctor in case of a municipality number</documentation>
			<documentation xml:lang="da-DK">Identifikationsnummer for den ordinerende læges organisation såfremt dette er et kommunenummer</documentation>
		</annotation>
	</element>
	<simpleType name="MunicipalityOrganisationIdentifierType">
		<restriction base="int">
			<minInclusive value="100"/>
			<maxExclusive value="999"/>
		</restriction>
	</simpleType>
</schema>
