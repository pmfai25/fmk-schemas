			<element ref="medicinecard:VersionMismatchWarningIndicator" minOccurs="0"/>						
			<element ref="medicinecard:CreatedEffectuationOnDrugMedicationStructure" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>
