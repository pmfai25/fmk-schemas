<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="DrugMedicationTypeIdentifier" type="medicinecard:DrugMedicationTypeIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">The type of drug medication</documentation>
			<documentation xml:lang="da-DK">Ordinationens type, f.eks. fast, efter behov eller engangsdosis</documentation>
		</annotation>
	</element>
	<simpleType name="DrugMedicationTypeIdentifierType">
		<restriction base="string">
			<enumeration value="fast"/>
			<enumeration value="efter behov"/>
			<enumeration value="engangsdosis"/>
		</restriction>
	</simpleType>
</schema>
