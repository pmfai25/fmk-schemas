	<include schemaLocation="DKMA_DeliveryInformationText.xsd"/>
	<element name="DeliveryInformationStructure" type="medicinecard:DeliveryInformationStructureType">
		<annotation>
			<documentation xml:lang="en-GB">1 to 3 lines of text for delivery information</documentation>
			<documentation xml:lang="da-DK">1 til 3 liniers tekst til leveringsinformation</documentation>
		</annotation>
