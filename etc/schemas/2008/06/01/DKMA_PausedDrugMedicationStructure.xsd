<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DKMA_DrugMedicationIdentifier.xsd"/>
	<include schemaLocation="DKMA_DrugMedicationVersionIdentifier.xsd"/>
	<element name="PausedDrugMedicationStructure" type="medicinecard:PausedDrugMedicationStructureType">
		<annotation>
			<documentation xml:lang="en-GB">Element with IDs returned when a drug prescription has been paused using a batch request</documentation>
			<documentation xml:lang="da-DK">Element med IDer returneret når en lægemiddelinformation er blevet pauseret via en batchforespørgsel</documentation>
		</annotation>
	</element>
	<complexType name="PausedDrugMedicationStructureType">
		<sequence>
			<element ref="medicinecard:DrugMedicationIdentifier"/>
			<element ref="medicinecard:DrugMedicationVersionIdentifier"/>
		</sequence>
	</complexType>
</schema>
