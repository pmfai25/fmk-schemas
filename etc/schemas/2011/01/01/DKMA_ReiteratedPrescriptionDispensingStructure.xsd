			<element ref="medicinecard20080601:ReiterationInterval"/>
			<element ref="medicinecard20080601:ReiterationIntervalUnitText"/>
			<element ref="medicinecard20080601:PackageQuantity"/>
			<element ref="medicinecard20110101:DosageCode" minOccurs="0"/>
