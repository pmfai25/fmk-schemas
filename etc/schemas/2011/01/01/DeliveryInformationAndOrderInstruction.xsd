<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard="http://www.dkma.dk/medicinecard/xml.schema/2011/01/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2011/01/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="DeliveryInformationAndOrderInstruction" type="medicinecard:DeliveryInformationAndOrderInstructionType">
		<annotation>
			<documentation xml:lang="en-GB">A text line for either delivery information or order instruction</documentation>
			<documentation xml:lang="da-DK">En linies tekst til enten leveringsinformation eller ordre instruktion</documentation>
		</annotation>
	</element>
	<simpleType name="DeliveryInformationAndOrderInstructionType">
		<restriction base="string">
			<minLength value="1"/>
			<maxLength value="70"/>
		</restriction>
	</simpleType>
</schema>
