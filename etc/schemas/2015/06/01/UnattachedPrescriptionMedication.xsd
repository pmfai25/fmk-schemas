	<include schemaLocation="SubstitutionAllowed.xsd"/>
	<include schemaLocation="SupplementaryInformation.xsd"/>
	<include schemaLocation="SystemName.xsd"/>
	<element name="UnattachedPrescriptionMedication" type="medicinecard20150601:UnattachedPrescriptionMedicationType">
		<annotation>
			<documentation xml:lang="en-GB">Request document for creating an unattached PrescriptionMedication.</documentation>
			<documentation xml:lang="da-DK">Request-dokument til at oprette en l√∏s receptordination.</documentation>
		</annotation>
	</element>
	<complexType name="UnattachedPrescriptionMedicationType">
		<sequence>
			<element name="CreatedBy" type="medicinecard20150601:ModificatorType" minOccurs="0"/>
			<element name="ModificationMetadata" type="medicinecard20150601:ModificationMetadataType" minOccurs="0"/>			
			<element name="AuthorisationDateTime" type="medicinecard20150601:AuthorisationDateTimeType"/>
			<element name="SystemName" type="medicinecard20150601:SystemNameType"/>
            <element name="ValidFromDate" type="medicinecard20140601:ValidDateType" minOccurs="0"/>
            <element name="ValidToDate" type="medicinecard20140601:ValidDateType"/>
            <element name="ReimbursementClause" type="medicinecard20150601:ReimbursementClauseType" minOccurs="0"/>
            <element name="Dosage" type="medicinecard20150601:DosageType" minOccurs="0"/>
            <choice>
                <element name="PackageRestriction" type="medicinecard20140601:PackageRestrictionType"/>
                <element name="DoseDispensedRestriction" type="medicinecard20140601:DoseDispensedRestrictionType"/>
			</choice>
			<element name="Indication" type="medicinecard20150601:IndicationType" minOccurs="0"/>
			<element name="SubstitutionAllowed" type="medicinecard20150601:SubstitutionAllowedType"/>
			<element name="SupplementaryInformation" type="medicinecard20150601:SupplementaryInformationType" minOccurs="0"/>
            <element name="Order" type="medicinecard20150601:OrderType" minOccurs="0"/>
		</sequence>
	</complexType>
