<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150601="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <include schemaLocation="IncludeEffectuations.xsd"/>
    <include schemaLocation="IncludeNonRelevantPrescriptionMedications.xsd"/>
    <include schemaLocation="IncludePrescriptionMedications.xsd"/>
    <include schemaLocation="MedicineCardVersion.xsd"/>
    <include schemaLocation="ModifiedDateTime.xsd"/>
    <include schemaLocation="NegativeConsentRequest.xsd"/>
    <include schemaLocation="PersonIdentifier.xsd"/>
    <element name="GetMedicineCardRequest" type="medicinecard20150601:GetMedicineCardRequestType">
        <annotation>
            <documentation xml:lang="en-GB">Element for a request for a medicinecard</documentation>
            <documentation xml:lang="da-DK">Element for en foresp√∏rgsel efter et medicinkort</documentation>
        </annotation>
    </element>
    <complexType name="GetMedicineCardRequestType">
        <sequence>
            <element name="PersonIdentifier" type="medicinecard20150601:PersonIdentifierType"/>
            <element name="Version" type="medicinecard20150601:MedicineCardVersionType" minOccurs="0" maxOccurs="unbounded"/>
            <element name="DateTime" type="medicinecard20150601:ModifiedDateTimeType" minOccurs="0" maxOccurs="unbounded"/>
            <element name="NegativeConsent" type="medicinecard20150601:NegativeConsentRequestType" minOccurs="0"/>
            <choice>
                <sequence>
                    <element name="IncludePrescriptionMedications" type="medicinecard20150601:IncludePrescriptionMedicationsType"/>
                    <element name="IncludeEffectuations" type="medicinecard20150601:IncludeEffectuationsType"/>
                    <element name="IncludeNonRelevantPrescriptionMedications" type="medicinecard20150601:IncludeNonRelevantPrescriptionMedicationsType"/>
                </sequence>
                <choice>
                    <element name="IncludeOnlyEffectuatableContent" type="medicinecard20150601:IncludeOnlyEffectuatableContentType"/>   <!-- Ikke boolean, men bare element -->
                    <element name="IncludeOnlyDoseDispensableContent" type="medicinecard20150601:IncludeOnlyDoseDispensableContentType"/> <!-- Ikke boolean, men bare element -->
                </choice>
            </choice>
            <element name="CompleteConsentGiven" type="medicinecard20150601:CompleteConsentGivenType" minOccurs="0"/>
        </sequence>
    </complexType>
</schema>
