<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150601="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="Delivery.xsd"/>
	<include schemaLocation="DeliveryInformation.xsd"/>
	<include schemaLocation="DrugMedicationIdentifier.xsd"/>
	<include schemaLocation="ModificationMetadata.xsd"/>
	<include schemaLocation="Modificator.xsd"/>
	<include schemaLocation="OrderInstruction.xsd"/>
	<include schemaLocation="Organisation.xsd"/>
	<include schemaLocation="PrescriptionIdentifier.xsd"/>
	<element name="OrderEffectuation" type="medicinecard20150601:OrderEffectuationType">
		<annotation>
			<documentation xml:lang="en-GB">Order for a new effectuation on the pharmacy</documentation>
			<documentation xml:lang="da-DK">Bestilling for en en ny effektuering p√• apoteket</documentation>
		</annotation>
	</element>
	<complexType name="OrderEffectuationType">
		<sequence>
			<element name="DrugMedicationIdentifier" type="medicinecard20150601:DrugMedicationIdentifierType"/>
			<element name="PrescriptionIdentifier" type="medicinecard20150601:PrescriptionIdentifierType" minOccurs="0"/>
			<element name="OrderedBy" type="medicinecard20150601:ModificatorType" minOccurs="0"/>
			<element name="ModificationMetadata" type="medicinecard20150601:ModificationMetadataType" minOccurs="0"/>
			<element name="EffectuatingOrganisation" type="medicinecard20150601:OrganisationType"/>
			<choice minOccurs="0" maxOccurs="3">
				<element name="DeliveryInformation" type="medicinecard20150601:DeliveryInformationType"/>
				<element name="OrderInstruction" type="medicinecard20150601:OrderInstructionType"/>
			</choice>
			<element name="Delivery" type="medicinecard20150601:DeliveryType" minOccurs="0"/>
		</sequence>
	</complexType>
</schema>
