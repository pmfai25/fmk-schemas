    <include schemaLocation="importsFrom20150601E3.xsd"/>

	<complexType name="DrugSubstitutionType">
		<annotation>
			<documentation xml:lang="en-GB">Specification of pharmacy substitution</documentation>
			<documentation xml:lang="da-DK">Detaljer vedr√∏rende apotekets substitution</documentation>
		</annotation>
		<sequence>
			<choice>
				<element name="SubstitutionChosenByPharmacy" type="medicinecard20150601E3:SubstitutionChosenByPharmacyType"/>
