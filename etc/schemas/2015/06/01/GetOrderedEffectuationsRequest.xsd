<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150601="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <include schemaLocation="IncludeOrderedEffectuations.xsd"/>
    <include schemaLocation="IncludeOrderedPrescriptions.xsd"/>
    <include schemaLocation="OrderedDateTime.xsd"/>
    <include schemaLocation="OrderedEffectuationIdentifier.xsd"/>
    <include schemaLocation="OrderEffectuationOrPrescription.xsd"/>
    <include schemaLocation="PersonIdentifier.xsd"/>
    <element name="GetOrderedEffectuationsRequest" type="medicinecard20150601:GetOrderedEffectuationsRequestType">
        <annotation>
            <documentation xml:lang="en-GB">Get ordered effectuations</documentation>
            <documentation xml:lang="da-DK">Opslag p√• bestilte udleveringer</documentation>
        </annotation>
    </element>
    <complexType name="GetOrderedEffectuationsRequestType">
        <sequence>
            <element name="PersonIdentifier" type="medicinecard20150601:PersonIdentifierType"/>
            <element name="FromDateTime" type="medicinecard20150601:OrderedDateTimeType" minOccurs="0"/>
