<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150601="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="StatusInformation" type="medicinecard20150601:StatusInformationType">
		<annotation>
			<documentation xml:lang="en-GB">A text line for extra status information, e.g. "ordered at supplier" or "release is pending products returned from delivery site."</documentation>
			<documentation xml:lang="da-DK">En linies tekst til ekstra statusinformation, f.eks. "bestilles hjem" eller "frigivelse afventer returvare fra forsendelsessted".</documentation>
		</annotation>
	</element>
	<simpleType name="StatusInformationType">
		<restriction base="string">
			<minLength value="1"/>
			<maxLength value="70"/>
		</restriction>
	</simpleType>
</schema>
