		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01"
		elementFormDefault="qualified" attributeFormDefault="unqualified">
	<include schemaLocation="DrugMedicationIdentifier.xsd"/>
	<include schemaLocation="DrugMedicationVersion.xsd"/>
	<include schemaLocation="UpdateOrder_PrescriptionMedication.xsd"/>

	<complexType name="UpdateOrder_DrugMedicationAndPrescriptionMedicationType">
