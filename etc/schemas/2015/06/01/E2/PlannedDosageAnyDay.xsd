<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
		xmlns:medicinecard20150601E2="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01/E2" 
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/06/01/E2" 
		elementFormDefault="qualified" 
		attributeFormDefault="unqualified">

	<include schemaLocation="PlannedDose.xsd"/>
	<complexType name="PlannedDosageAnyDayType">
		<annotation>
			<documentation xml:lang="en-GB">Group of planned dosage times for any day</documentation>
			<documentation xml:lang="da-DK">Gruppering af planlagte doseringstidspunkter på en vilkårlig dag</documentation>
		</annotation>
		<sequence>
			<element name="Dose" type="medicinecard20150601E2:PlannedDoseType" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>
