<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E4"
        elementFormDefault="qualified"
        attributeFormDefault="unqualified">

	<simpleType name="PackagedDateTimeType">
		<annotation>
			<documentation xml:lang="en-GB">Packed date time for a dose dispensing period</documentation>
			<documentation xml:lang="da-DK">Pakketidspunkt for en dosisdispenseringsperiode</documentation>
		</annotation>
		<restriction base="dateTime">
			<minInclusive value="2000-01-01T00:00:00Z"/>
		</restriction>
	</simpleType>
</schema>
