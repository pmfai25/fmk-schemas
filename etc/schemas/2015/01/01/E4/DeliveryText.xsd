<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E4"
        elementFormDefault="qualified"
        attributeFormDefault="unqualified">
		
    <simpleType name="DeliveryTextType">
        <annotation>
            <documentation xml:lang="en-GB">Free text delivery text for dosedispensing</documentation>
            <documentation xml:lang="da-DK">Fritekst leveringsoplysninger for dosisdispensering</documentation>
        </annotation>
        <restriction base="string">
            <minLength value="1"/>
            <maxLength value="300"/>
        </restriction>
    </simpleType>
</schema>
