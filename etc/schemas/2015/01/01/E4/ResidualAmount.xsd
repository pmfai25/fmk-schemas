<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E4"
		elementFormDefault="qualified"
		attributeFormDefault="unqualified">
	<simpleType name="ResidualAmountType">
		<annotation>
			<documentation xml:lang="en-GB">The amount remaning on a prescription. Unit not given on this element</documentation>
			<documentation xml:lang="da-DK">Den resterende mængde på en recept. Enhed er ikke angivet på dette element</documentation>
		</annotation>
		<restriction base="decimal">
			<fractionDigits value="2"/>
			<totalDigits value="10"/>
			<minInclusive value="0.00"/>
		</restriction>
	</simpleType>
</schema>
