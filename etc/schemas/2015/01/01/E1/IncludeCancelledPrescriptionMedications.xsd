<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:medicinecard20150101E1="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E1"
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E1"
		elementFormDefault="qualified"
		attributeFormDefault="unqualified">
	<element name="IncludeCancelledPrescriptionMedications" type="medicinecard20150101E1:IncludeCancelledPrescriptionMedicationsType">
		<annotation>
			<documentation xml:lang="en-GB">Indicates if cancelled prescription medication orders should be included in the response</documentation>
			<documentation xml:lang="da-DK">Angiver hvorvidt aflyste receptordinationer skal inkluderes i svaret</documentation>
		</annotation>
	</element>
	<simpleType name="IncludeCancelledPrescriptionMedicationsType">
		<restriction base="boolean"/>
	</simpleType>
</schema>
