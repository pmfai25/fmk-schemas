<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		xmlns:medicinecard20150101E2="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E2"
		xmlns:medicinecard20150101E5="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E5"
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E5" 
		elementFormDefault="qualified" 
		attributeFormDefault="unqualified">

	<import schemaLocation="importsFrom20150101E2.xsd" namespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E2"/>
	<include schemaLocation="importsFrom20150101E5.xsd"/>

	<element name="PatientWithOrders" type="medicinecard20150101E5:PatientWithOrdersType">
		<annotation>
			<documentation xml:lang="en-GB">Patient identifier with orders sorted by status</documentation>
			<documentation xml:lang="da-DK">Patient identifikation med bestillinger sorteret per status</documentation>
		</annotation>
	</element>
	<complexType name="PatientWithOrdersType">
		<sequence>
			<element name="PersonIdentifier" type="medicinecard20150101E2:PersonIdentifierType"/>
			<element name="UnhandledOrders" type="medicinecard20150101E5:PatientOrdersType" minOccurs="0"/>
			<element name="HandledOrders" type="medicinecard20150101E5:PatientOrdersType" minOccurs="0"/>
			<element name="CancelledOrders" type="medicinecard20150101E5:PatientOrdersType" minOccurs="0"/>
		</sequence>
	</complexType>
</schema>
