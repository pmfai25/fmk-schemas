<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E2" xmlns:medicinecard20150101E2="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E2" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<import schemaLocation="importsFrom20150101.xsd" namespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"/>
	<import schemaLocation="importsFrom20150101E1.xsd" namespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E1"/>
	<include schemaLocation="importsFrom20150101E2.xsd"/>
	<element name="GetOrderedEffectuationSummaryRequest" type="medicinecard20150101E2:GetOrderedEffectuationSummaryRequestType">
		<annotation>
			<documentation xml:lang="en-GB">Get a summary of the ordered effectuations</documentation>
			<documentation xml:lang="da-DK">Opslag på bestilte udleveringer på opsummeret form</documentation>
		</annotation>
	</element>
	<complexType name="GetOrderedEffectuationSummaryRequestType">
		<sequence>
			<choice>
				<sequence>
					<element name="PersonIdentifier" type="medicinecard20150101E2:PersonIdentifierType" />
					<element name="FromDateTime" type="medicinecard20150101:OrderedDateTimeType" minOccurs="0"/>
					<element name="ToDateTime" type="medicinecard20150101:OrderedDateTimeType" minOccurs="0"/>
					<choice minOccurs="0">
						<element name="IncludeOnly">
							<complexType>
								<sequence>
									<element name="Identifier" type="medicinecard20150101:OrderedEffectuationIdentifierType" maxOccurs="unbounded"/>
								</sequence>
							</complexType>
						</element>
						<element name="Exclude">
							<complexType>
								<sequence>
									<element name="Identifier" type="medicinecard20150101:OrderedEffectuationIdentifierType" maxOccurs="unbounded"/>
								</sequence>
							</complexType>
						</element>
					</choice>
				</sequence>
				<sequence>
					<choice>
						<element name="OrderingOrganisation" type="medicinecard20150101:OrganisationType"/>
						<element name="PrescribingOrganisation" type="medicinecard20150101:OrganisationType"/>
					</choice>
					<element name="FromDateTime" type="medicinecard20150101:OrderedDateTimeType" minOccurs="0"/>
					<element name="ToDateTime" type="medicinecard20150101:OrderedDateTimeType" minOccurs="0"/>
				</sequence>
			</choice>
		</sequence>
	</complexType>
</schema>
