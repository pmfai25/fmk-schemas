<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
		xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" 
		xmlns:medicinecard20150101E2="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E2" 
		targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01/E2" 
		elementFormDefault="qualified" 
		attributeFormDefault="unqualified">

	<import  schemaLocation="../PersonIdentifier.xsd" namespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"/>
	<import  schemaLocation="../Modificator.xsd" namespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01"/>
	<element name="ResumeDoseDispensingRequest" type="medicinecard20150101E2:ResumeDoseDispensingRequestType"/>

	<complexType name="ResumeDoseDispensingRequestType">
		<annotation>
			<documentation xml:lang="en-GB">Element for requesting dose dispensing be resumed for a specific patient if it was on hold</documentation>
			<documentation xml:lang="da-DK">Element for en anmodning om at gentoptage dosisdispensering for en given patient hvis det var sat i bero</documentation>
		</annotation>
		<sequence>
			<element name="PersonIdentifier" type="medicinecard20150101:PersonIdentifierType"/>
			<element name="ModifiedBy" type="medicinecard20150101:ModificatorType" />
			<element name="ReportedBy" type="medicinecard20150101:ModificatorType" minOccurs="0"/>
		</sequence>
	</complexType>
</schema>
