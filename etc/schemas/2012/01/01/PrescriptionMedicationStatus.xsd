<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:medicinecard20120101="http://www.dkma.dk/medicinecard/xml.schema/2012/01/01" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2012/01/01" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="PrescriptionMedicationStatus" type="medicinecard20120101:PrescriptionMedicationStatusType">
		<annotation>
			<documentation xml:lang="en-GB">The status of the prescription medication</documentation>
			<documentation xml:lang="da-DK">Receptens status</documentation>
		</annotation>
	</element>
	<simpleType name="PrescriptionMedicationStatusType">
		<restriction base="string">
			<enumeration value="Open" />
			<enumeration value="Ended" />
			<enumeration value="Cancelled" />
			<enumeration value="Inactive" />
			<enumeration value="Draft" />
			<enumeration value="PartiallyDelivered" />
			<enumeration value="BeingProcessed" />
			<enumeration value="Invalidated" />
			<enumeration value="WebHandled" />
			<enumeration value="TransferredToDosageCard" />
		</restriction>
	</simpleType>
</schema>
