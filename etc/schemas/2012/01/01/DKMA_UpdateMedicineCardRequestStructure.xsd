			<element ref="medicinecard20080601:SuspendMedicineCardStructure" minOccurs="0"/>
			<element ref="medicinecard20090101:ResuspendMedicineCardStructure" minOccurs="0"/>
			<element ref="medicinecard20080601:UnsuspendMedicineCardStructure" minOccurs="0"/>
