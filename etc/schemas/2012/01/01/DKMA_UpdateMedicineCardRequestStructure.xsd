			<element ref="medicinecard20090101:CreateEffectuationOnDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:DetachPrescriptionMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:AttachPrescriptionMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20110101:CreatePrescriptionMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20120101:DeleteEffectuationStructure" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
