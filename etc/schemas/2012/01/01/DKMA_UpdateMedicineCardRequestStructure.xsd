	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2011/01/01" schemaLocation="../../../2011/01/01/DKMA_CreatePrescriptionMedicationStructure.xsd"/>
	<include schemaLocation="DeleteEffectuationStructure.xsd"/>
	<include schemaLocation="UnWithdrawDrugMedicationStructure.xsd"/>
	<include schemaLocation="DKMA_UpdateDrugMedicationStructure.xsd" />
	<element name="UpdateMedicineCardRequestStructure" type="medicinecard20120101:UpdateMedicineCardRequestStructureType">
		<annotation>
			<documentation xml:lang="en-GB">Outer container element for a batch request to update a medicine card</documentation>
			<documentation xml:lang="da-DK">Ydre container-element for en batchforesp√∏rgsel for at opdatere et medicinkort</documentation>
		</annotation>
	</element>
	<complexType name="UpdateMedicineCardRequestStructureType">
		<sequence>
			<element ref="cpr:PersonCivilRegistrationIdentifier"/>
			<element ref="medicinecard20080601:MedicineCardVersionIdentifier"/>
			<element ref="medicinecard20090101:OrganisationStructure"/>
			<element ref="medicinecard20080601:DoctorStructure" minOccurs="0"/>
			<element ref="medicinecard20080601:SetMedicineCardReviewedStructure" minOccurs="0"/>
