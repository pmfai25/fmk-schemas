<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
    xmlns:medicinecard20120101="http://www.dkma.dk/medicinecard/xml.schema/2012/01/01"
	xmlns:medicinecard20090101="http://www.dkma.dk/medicinecard/xml.schema/2009/01/01" 
	xmlns:medicinecard20080601="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" 
	xmlns:cpr="http://rep.oio.dk/cpr.dk/xml/schemas/core/2005/03/18/" 
	targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2012/01/01" 
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<import namespace="http://rep.oio.dk/cpr.dk/xml/schemas/core/2005/03/18/" schemaLocation="http://rep.oio.dk/cpr.dk/xml/schemas/core/2005/03/18/CPR_PersonCivilRegistrationIdentifier.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_MedicineCardVersionIdentifier.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_VersionMismatchWarningIndicator.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_UpdatedDrugMedicationStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_WithdrawnDrugMedicationStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_PausedDrugMedicationStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_UnpausedDrugMedicationStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_CreatedEffectuationOnDrugMedicationStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2008/06/01" schemaLocation="../../../2008/06/01/DKMA_CreatedPrescriptionMedicationStructure.xsd"/>
	<import namespace="http://www.dkma.dk/medicinecard/xml.schema/2009/01/01" schemaLocation="../../../2009/01/01/DKMA_CreatedDrugMedicationStructure.xsd"/>
	<include schemaLocation="UnWithdrawnDrugMedication.xsd"/>
	<include schemaLocation="CancelledPrescriptionMedication.xsd"/>
	<element name="UpdateMedicineCardResponseStructure" type="medicinecard20120101:UpdateMedicineCardResponseStructureType">
		<annotation>
			<documentation xml:lang="en-GB">Outer container elemenet for a batch request to update a medicine card</documentation>
			<documentation xml:lang="da-DK">Ydre container-element for en batchforesp√∏rgsel for at opdatere et medicinkort</documentation>
		</annotation>
	</element>
	<complexType name="UpdateMedicineCardResponseStructureType">
		<sequence>
			<element ref="cpr:PersonCivilRegistrationIdentifier"/>
			<element ref="medicinecard20080601:MedicineCardVersionIdentifier"/>
			<element ref="medicinecard20080601:VersionMismatchWarningIndicator" minOccurs="0"/>
			<element ref="medicinecard20090101:CreatedDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:UpdatedDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:WithdrawnDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:PausedDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:UnpausedDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:CreatedEffectuationOnDrugMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20080601:CreatedPrescriptionMedicationStructure" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20120101:UnWithdrawnDrugMedication" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="medicinecard20120101:CancelledPrescriptionMedication" minOccurs="0"/>
		</sequence>
	</complexType>
</schema>
