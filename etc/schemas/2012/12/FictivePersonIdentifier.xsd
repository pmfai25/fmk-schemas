<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:reimbursement20121201="http://nsi.dk/reimbursement/xml.schema/2012/12/01"
	targetNamespace="http://nsi.dk/reimbursement/xml.schema/2012/12/01"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="FictivePersonIdentifier" type="reimbursement20121201:FictivePersonIdentifierType">
		<annotation>
			<documentation xml:lang="en-GB">Element for a person
				identifier, according to the Danish CTR format for fictitious CPR
				numbers.
			</documentation>
			<documentation xml:lang="da-DK">Element for et person
				identifikations nummer, der f√∏lger CTRs standard for fiktive
				CPR-nummre.
			</documentation>
		</annotation>
	</element>
	<simpleType name="FictivePersonIdentifierType">
		<restriction base="string">
			<pattern value="((4|5)[0-9]{9})" />
		</restriction>
	</simpleType>
</schema>