<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:reimbursement20121201="http://nsi.dk/reimbursement/xml.schema/2012/12/01"
	targetNamespace="http://nsi.dk/reimbursement/xml.schema/2012/12/01"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<element name="TelephoneNumber" type="reimbursement20121201:TelephoneNumberType">
		<annotation>
			<documentation xml:lang="en">Telephone number including international prefix</documentation>
			<documentation xml:lang="da">Telefonnummer inkl. internationalt prefix</documentation>
		</annotation>
	</element>
	<simpleType name="TelephoneNumberType">
		<restriction base="string">
			<pattern value="(\+)?[0-9]{3,20}"/>
		</restriction>
	</simpleType>
</schema>
