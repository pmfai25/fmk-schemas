						<element name="EmailAddress" type="medicinecard20120601:EmailAddressType" minOccurs="0"/>
						<element name="Type" type="medicinecard20120601:OrganisationTypeType" minOccurs="0"/>
						<element name="Identifier" type="medicinecard20120601:OrganisationIdentifierType" minOccurs="0"/>
					</sequence>
				</complexType>
			</element>
			<element name="Address" minOccurs="0">
				<complexType>
					<sequence>
						<element name="MailDeliverySublocationIdentifier" type="medicinecard20120601:MailDeliverySublocationIdentifierType" minOccurs="0"/>
