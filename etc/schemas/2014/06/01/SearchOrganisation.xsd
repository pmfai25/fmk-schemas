		<sequence>
			<element name="Organisation" minOccurs="0">
				<complexType>
					<sequence>
						<element name="Name" type="medicinecard20120601:OrganisationNameType" minOccurs="0"/>
						<element name="AddressLine" type="medicinecard20120601:AddressLineType" minOccurs="0" maxOccurs="5"/>
